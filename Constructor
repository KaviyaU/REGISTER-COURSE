class Car {
    String brand;
    String color;

    
    Car() {
        brand = "Honda";
        color = "Black";
    }

    
    void display() {
        System.out.println("Car Brand: " + brand);
        System.out.println("Car Color: " + color);
    }
}

public class DefaultConstructorExample {
    public static void main(String[] args) {
        
        Car myCar = new Car();

       
        myCar.display();
    }
}
